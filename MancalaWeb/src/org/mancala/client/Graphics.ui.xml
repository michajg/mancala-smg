<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui">
  <ui:style field="css" type="org.mancala.client.GameCss">
    .highlighted {
      outline: dotted green 2px;
    }
    td img {
      display: block
    }
  </ui:style>
  <g:HTMLPanel>
    <g:PopupPanel ui:field="gamePopupPanel"/>
  	<g:Label ui:field="turnLabel"/>
    <g:AbsolutePanel ui:field="gameAbsolutePanel"/>
    <g:Label ui:field="warnLabel"/>
    <g:Button ui:field="saveButton"/>
    <g:Button ui:field="loadButton"/>  
    <g:Label ui:field="dragMeLabel"/>   
    <g:Label ui:field="dragOnToMeLabel"/>  
  </g:HTMLPanel>
</ui:UiBinder> 